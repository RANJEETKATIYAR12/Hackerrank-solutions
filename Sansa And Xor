#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int arr[n];
        for (int i = 0; i < n; i++) {
            scanf("%d", &arr[i]);
        }
        
        int result = 0;
        if (n % 2 == 1) {  // only if n is odd
            for (int i = 0; i < n; i += 2) {
                result ^= arr[i];
            }
        }
        
        printf("%d\n", result);
    }
    return 0;
}
