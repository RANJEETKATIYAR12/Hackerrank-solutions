
#include<stdio.h>
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n,a,b,sum,h[1000000]={0};
        scanf("%d%d%d",&n,&a,&b);
        for(int i=0;i<n;i++)
        {
            sum= i*a+(n-i-1)*b;
            h[sum]++;
        }
        for(int i=0;i < 1000000;i++)
            if(h[i] != 0)
                printf("%d ",i);
        printf("\n");
    }
}
